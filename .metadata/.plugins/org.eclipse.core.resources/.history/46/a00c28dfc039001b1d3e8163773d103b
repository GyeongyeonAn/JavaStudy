package stream.serialization;

class Person {
	String name;
	String jop;
	
	public Person(String name, String jop) {
		this.name = name;
		this.jop = jop;
	}
	
	public String toString() {
		return name + ", " + jop;
	}
	
}

public class SerializationTest {

	public static void main(String[] args) {

		Person personLee = new Person("이순신", "엔지니어")
		
	}

}
