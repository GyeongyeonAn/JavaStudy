package exception;

public class IDFormatTest {

	private String userID;
	
	public String getUserID() {
		return userID;
	}

	public void setUserID(String userID) {
		
		if(userID == null) {
			throw new IDFormatException("아ㅇ디는 null일 수 없습니다.");
		}
		
		this.userID = userID;
	}

	public static void main(String[] args) {
		
		
	}

}
