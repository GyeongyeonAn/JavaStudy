package stream.inputstream;

import java.io.IOException;

public class SystemInTest2 {

	public static void main(String[] args) {

		System.out.println("입력 후 끝이라고 쓰세요.: ");
		
		try {
			int i;
			while((i = System.in.read()) != '끝') {
				System.out.print((char)i);
			}
		} catch (IOException e) {
			e.printStackTrace();
		}
		
	}

}
